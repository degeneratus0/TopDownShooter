;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Shoot 'em"
run/main_scene="res://UI/MainMenu.tscn"
config/features=PackedStringArray("4.2", "C#")
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Shoot -em"

[editor]

naming/default_signal_callback_name="On{NodeName}{SignalName}"
naming/default_signal_callback_to_self_name="On{SignalName}"
version_control_autoload_on_startup=true
version_control_plugin_name="GitAPI"

[global]

gravity=false

[gui]

